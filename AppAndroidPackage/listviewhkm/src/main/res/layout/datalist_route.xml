<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
			  android:layout_width="match_parent"
			  android:layout_height="match_parent"
			  android:orientation="vertical">

   <LinearLayout
	  android:layout_width="match_parent"
	  android:layout_height="wrap_content"
	  android:orientation="horizontal">

	  <TextView
		 android:id="@+id/route_label"
		 android:layout_width="wrap_content"
		 android:layout_height="wrap_content"
		 style="@style/c.column.label"/>

	  <TextView
		 android:id="@+id/column1"
		 android:layout_width="wrap_content"
		 android:layout_height="wrap_content"
		 style="@style/c.column.dep"
		 />

	  <TextView
		 android:id="@+id/column2"
		 android:layout_width="wrap_content"
		 android:layout_height="wrap_content"
		 android:text="@string/row_d_a"
		 style="@style/c.column"
		 />

	  <TextView
		 android:id="@+id/column3"
		 android:layout_width="wrap_content"
		 android:layout_height="wrap_content"
		 android:text="@string/row_d_b"
		 style="@style/c.column"
		 />

	  <TextView
		 android:id="@+id/column4"
		 android:layout_width="wrap_content"
		 android:layout_height="wrap_content"
		 style="@style/c.column.cable_radius.label"
		 />
   </LinearLayout>

   <LinearLayout
	  android:id="@+id/mlist_container"
	  android:layout_width="fill_parent"
	  android:layout_height="fill_parent"
	  android:orientation="vertical"
	  >
   </LinearLayout>
</LinearLayout>